{
  "engines": {
    "node": "18.x",
    "npm": "9.5.x"
  },
  "private": true,
  "scripts": {
    "build": "node deployment/loadContent.mjs && nuxt build",
    "start": "nuxt start",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "test": "concurrently -c \"auto\" --names \"RUN,TEST\" --kill-others -s first \"npm run start\" \"npm run cy:run\"",
    "lint-all": "eslint .",
    "lint-fix-all": "eslint . --fix",
    "prepare": "husky install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "load-content": "node deployment/loadContent.mjs",
    "build-and-test": "npm run build && npm run test",
    "static-export": "node deployment/loadContent.mjs && nuxt generate"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
  },
  "devDependencies": {
    "@mdi/font": "^7.0.96",
    "@mdi/js": "^7.0.96",
    "@nuxt/content": "^2.8.2",
    "@nuxt/devtools": "^0.8.2",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/i18n": "^8.0.0-rc.4",
    "@vitejs/plugin-vue": "^3.2.0",
    "concurrently": "^7.6.0",
    "cypress": "^12.5.1",
    "eslint": "^8.28.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "sass": "^1.60.0",
    "swagger-typescript-api": "^12.0.2",
    "vite-plugin-vuetify": "^1.0.0",
    "vuetify": "^3.*"
  },
  "dependencies": {
    "@nuxt/image": "^1.0.0-rc.1",
    "@types/leaflet": "^1.9.0",
    "leaflet": "^1.9.2",
    "nuxt": "^3.7.0"
  }
}
